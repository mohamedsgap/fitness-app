{
  "expo": {
    "name": "Fitness App",
    "description": "The best triathlon tracker ever",
    "githubUrl": "https://github.com/mohamedsgap/fitness-app",
    "slug": "udacfitness",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/udacifitness-512.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/udacifitness-45.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "org.fitness.exp"
    },
    "android": {
      "package": "org.fitness.exp"
    }
  }
}